{
  "name": "gatsby-starter-default",
  "description": "Gatsby default starter",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "gatsby": "^1.9.247",
    "gatsby-image": "^1.0.38",
    "gatsby-link": "^1.6.40",
    "gatsby-plugin-netlify-cms": "^1.0.8",
    "gatsby-plugin-react-helmet": "^2.0.10",
    "gatsby-plugin-sharp": "^1.6.31",
    "gatsby-plugin-styled-components": "^2.0.7",
    "gatsby-source-filesystem": "^1.5.21",
    "gatsby-transformer-remark": "^1.7.33",
    "gatsby-transformer-sharp": "^1.6.20",
    "react": "~16.4.1",
    "react-dom": "~16.4.1",
    "react-helmet": "^5.2.0",
    "styled-components": "^3.1.6",
    "tinycolor2": "~1.4.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "lint": "eslint . '**/.*.js' --cache || true",
    "format": "prettier-eslint --write --ignore **/node_modules/** \"**/*.{js,jsx}\"",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "deploy": "gatsby build && gh-pages -d public -b master",
    "storybook": "start-storybook -p 9001 -c storybook"
  },
  "devDependencies": {
    "@storybook/addon-options": "3.2.12",
    "@storybook/react": "3.2.12",
    "babel-eslint": "~8.2.3",
    "babel-jest": "~23.2.0",
    "babel-plugin-syntax-class-properties": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "enzyme": "~3.3.0",
    "enzyme-adapter-react-16": "~1.1.1",
    "eslint": "~4.19.1",
    "eslint-config-airbnb": "~16.1.0",
    "eslint-plugin-import": "~2.12.0",
    "eslint-plugin-jsx-a11y": "~6.0.3",
    "eslint-plugin-react": "~7.9.1",
    "gh-pages": "^1.1.0",
    "imports-loader": "0.7.1",
    "jest": "~23.2.0",
    "jest-styled-components": "~5.0.1",
    "prettier": "^1.12.0",
    "prettier-eslint-cli": "~4.7.1",
    "react-test-renderer": "~16.4.1"
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          "es2015",
          "react"
        ],
        "plugins": [
          "syntax-class-properties",
          "transform-class-properties"
        ]
      }
    }
  },
  "jest": {
    "transform": {
      ".(js|jsx)": "babel-jest"
    },
    "testRegex": "(\\.(test|spec))\\.(jsx|js)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/.cache/"
    ],
    "modulePaths": [
      "src"
    ],
    "moduleNameMapper": {
      "\\.(scss|svg|png|jpg)$": "<rootDir>/src/__tests__/styleMock.js"
    },
    "moduleFileExtensions": [
      "jsx",
      "js"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ],
    "setupTestFrameworkScriptFile": "./src/__tests__/setupTests.js"
  }
}
