{
  "name": "gatsby-starter-default",
  "description": "Gatsby default starter",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "dotenv": "^6.0.0",
    "gatsby": "^1.9.277",
    "gatsby-image": "^1.0.55",
    "gatsby-link": "^1.6.46",
    "gatsby-plugin-netlify-cms": "^1.0.12",
    "gatsby-plugin-react-helmet": "^2.0.10",
    "gatsby-plugin-sharp": "^1.6.31",
    "gatsby-plugin-styled-components": "^2.0.7",
    "gatsby-source-filesystem": "^1.5.21",
    "gatsby-transformer-remark": "^1.7.33",
    "gatsby-transformer-sharp": "^1.6.20",
    "hoist-non-react-statics": "^3.0.1",
    "react": "~16.4.2",
    "react-anchor-link-smooth-scroll": "^1.0.11",
    "react-dom": "~16.4.2",
    "react-helmet": "^5.2.0",
    "styled-components": "^3.4.2",
    "tinycolor2": "~1.4.1"
  },
  "keywords": [
    "gatsby",
    "styled components"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "lint": "eslint . '**/.*.js' --cache",
    "format": "prettier-eslint --write \"**/*.{js,jsx,json,md}\"",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "deploy": "gatsby build && gh-pages -d public -b master"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-syntax-class-properties": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "~4.19.1",
    "eslint-config-airbnb": "~16.1.0",
    "eslint-config-prettier": "^2.10.0",
    "eslint-plugin-import": "~2.12.0",
    "eslint-plugin-jsx-a11y": "~6.0.3",
    "eslint-plugin-react": "~7.9.1",
    "gh-pages": "^1.1.0",
    "husky": "^1.0.0-rc.13",
    "imports-loader": "0.7.1",
    "jest": "^23.6.0",
    "jest-styled-components": "^6.2.0",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.7",
    "prettier-eslint-cli": "~4.7.1",
    "react-test-renderer": "~16.4.2"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run format"
    }
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "arrowParens": "avoid",
    "trailingComma": "all"
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          "es2015",
          "react"
        ],
        "plugins": [
          "syntax-class-properties",
          "transform-class-properties"
        ]
      }
    }
  },
  "jest": {
    "transform": {
      ".(js|jsx)": "babel-jest"
    },
    "testRegex": "(\\.(test|spec))\\.(jsx|js)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/.cache/"
    ],
    "modulePaths": [
      "src"
    ],
    "moduleNameMapper": {
      "\\.(scss|svg|png|jpg)$": "<rootDir>/src/__tests__/styleMock.js"
    },
    "moduleFileExtensions": [
      "jsx",
      "js"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ],
    "setupTestFrameworkScriptFile": "./src/__tests__/setupTests.js"
  }
}
